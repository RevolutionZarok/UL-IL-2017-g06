%\newtoks\g@therbuf

\newcommand\gathersubscript{%
    \def\g@therbuf{\relax}%
    \futurelet\next\g@thersubscript%
}
\newcommand\g@thersubscript{%
    \def\g@thercont{}%
    \ifdigit\next{%
        \def\g@thercont{\g@th@rsubscript}%
    }%
    {%
        \if\g@therbuf\relax%
        \else%
            \def\g@thercont{%
                \ensuremath{{}_{\g@therbuf}}%
            }%
        \fi%
    }%
    \g@thercont%
}
\newcommand\g@th@rsubscript[1]{%
    \edef\g@therbuf{\g@therbuf#1}%
    \futurelet\next\g@thersubscript%
}

