\let\tosupdrop=\relax
\def\t@s@p#1#2#3{%
    \ifx#2\relax
        #3%
    \else%
        \smash{\raisebox{#2}{$#1{#3}$}}%
    \fi%
}
\def\t@sup#1#2{\t@s@p{#1}#2}
\newcommand\tosup[2][\tosupdrop]{\xrightarrow{\mathpalette\t@sup{{#1}{#2}}}}

